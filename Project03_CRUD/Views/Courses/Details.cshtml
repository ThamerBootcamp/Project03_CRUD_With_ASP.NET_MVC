@{
    var data = (CourseModel)ViewData["instructor"];
}


<div class="card" style="width: 18rem;">
    <div class="card-body">
        <h5 class="card-title">Name: @data.Name</h5>
        <p class="card-text">Price: @data.Price</p>
        <p class="card-text">Duration: @data.duration</p>
    </div>
    <ul class="list-group list-group-flush">
        <li class="list-group-item">Id: @data.Id</li>
        <li class="list-group-item">Publish Date @data.PublishDate</li>
    </ul>
</div>

<a asp-controller="Courses" asp-action="Index">Back to Instructors</a>